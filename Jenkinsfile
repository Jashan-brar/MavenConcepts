pipeline {
    agent any

    stages {
        stage('Start') {
            steps {
                echo 'Starting Jenkins pipeline...'
            }
        }

        stage('Test') {
            steps {
                echo 'Running tests...'
                echo 'Testing QA automation pipeline!'
            }
        }

        stage('End') {
            steps {
                echo 'Pipeline finished successfully!'
            }
        }
    }
}
